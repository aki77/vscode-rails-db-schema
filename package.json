{
  "name": "rails-db-schema",
  "displayName": "Rails DB Schema",
  "description": "Definition and Completion provider for Rails DB Schema.",
  "version": "0.2.5",
  "publisher": "aki77",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/aki77/vscode-rails-db-schema.git"
  },
  "bugs": {
    "url": "https://github.com/aki77/vscode-rails-db-schema/issues"
  },
  "activationEvents": [
    "onLanguage:ruby",
    "onLanguage:haml",
    "onLanguage:erb",
    "onLanguage:slim"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "railsDbSchema.open",
        "category": "Rails DB Schema",
        "title": "Open"
      },
      {
        "command": "railsDbSchema.insert",
        "category": "Rails DB Schema",
        "title": "Insert"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/inflection": "^1.5.28",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16",
    "@types/vscode": "^1.37.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.4.4",
    "vscode-test": "^1.0.2"
  },
  "dependencies": {
    "inflection": "^1.12.0",
    "sort-on": "^4.1.0"
  }
}
